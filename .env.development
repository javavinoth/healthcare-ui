# Healthcare Application - Development Environment Configuration

# API Configuration
# NOTE: Backend uses /api as servlet context path (server.servlet.context-path: /api)
# All endpoints automatically get /api prefix from Spring Boot
VITE_API_URL=https://f351c344d498.ngrok-free.app/api

# Application Info
VITE_APP_NAME=Healthcare Portal (Dev)
VITE_APP_VERSION=1.0.0

# Environment
VITE_ENVIRONMENT=development

# Security Settings
# Session timeout in milliseconds (5 minutes for dev testing)
VITE_SESSION_TIMEOUT=300000

# Feature Flags
VITE_ENABLE_2FA=true
VITE_ENABLE_BIOMETRIC_AUTH=false
VITE_ENABLE_TELEHEALTH=true

# Analytics (HIPAA-compliant only!)
# Do NOT use Google Analytics or similar without BAA
VITE_ENABLE_ANALYTICS=false

# Development Tools
VITE_ENABLE_REACT_QUERY_DEVTOOLS=true
VITE_ENABLE_DEBUG_LOGS=true
